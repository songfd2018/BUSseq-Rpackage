CXX_STD = CXX11
PKG_CXXFLAGS = $(SHLIB_OPENMP_CXXFLAGS)
PKG_LIBS = $(SHLIB_OPENMP_CXXFLAGS) $(LAPACK_LIBS) $(BLAS_LIBS) $(CXXLIBS)
SHLIB_LD = $(SHLIB_CXXLD)
SHLIB_LDFLAGS = $(SHLIB_CXXLDFLAGS)
SHLIB = pkgname$(SHLIB_EXT)
all: $(SHLIB)
$(SHLIB): $(OBJECTS)
	$(SHLIB_LINK) -o $(OBJECTS) $(ALL_LIBS) $(CXX_STD)
